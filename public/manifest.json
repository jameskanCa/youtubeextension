{
	"short_name": "Notetaker",
	"name": "Youtube Notes",
	"version": "0.1",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [ "*://www.youtube.com/*" ],
			"css": [ "/static/css/0.chunk.css", "/static/css/3.chunk.css", "/static/css/app.css" ],
			"js": [ "/static/js/0.chunk.js", "/static/js/3.chunk.js", "/static/js/content.js" ]
		}
	],
	"background": {
		"scripts": [ "background.js" ]
	},
	"permissions": [ "tabs", "webNavigation", "contextMenus", "*://youtube.com/*", "identity", "identity.email" ],
	"web_accessible_resources": [
		"/static/css/0.chunk.css",
		"/static/css/3.chunk.css",
		"/static/css/app.css",
		"/static/css/content.css",
		"*.html"
	],
	"content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
